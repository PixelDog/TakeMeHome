{
  "name": "pixel-dog/pet-search",
  "description": "A pet search",
  "license": "MIT",
  "keywords": [
    "pet",
    "search",
    "adopt-a-pet"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Keith Otto",
      "email": "kaozotto@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6",
    "illuminate/support": "~5.6.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.6.0",
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": {
      "PixelDog\\PetSearch\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PixelDog\\PetSearch\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "PixelDog\\PetSearch\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
